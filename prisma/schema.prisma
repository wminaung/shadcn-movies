generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Movie {
  id           String @id @default(cuid())
  title        String
  category     String
  release_year Int
  description  String
  rating       Float
  director     String
  runtime      Int
}
